{
  "manifest_version": 2,
  "name": "NOT PIXEL Bot V2.2.1",
  "version": "2.0.1",
  "description": "NOT PIXEL Bot V2.2.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["*"],
  "browser_action": {
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["http://web.telegram.org/*", "https://web.telegram.org/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
